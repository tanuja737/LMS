{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
  "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@/*": ["./*"]
  },
  "ignoreDeprecations": "6.0"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
